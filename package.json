{
	"name": "@kadence/helpers",
	"version": "1.1.0",
	"description": "Utility helpers shared by Kadence WordPress block plugins.",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js",
			"default": "./dist/esm/index.js"
		},
		"./package.json": "./package.json"
	},
	"sideEffects": false,
	"files": [
		"dist",
		"src",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"prettier": "prettier . --write",
		"clean": "rimraf dist",
		"build:esm": "babel src --out-dir dist/esm --env-name esm --extensions \".js,.jsx\" --source-maps",
		"build:cjs": "babel src --out-dir dist/cjs --env-name cjs --extensions \".js,.jsx\" --source-maps",
		"build": "npm run clean && npm run build:esm && npm run build:cjs",
		"prepare": "npm run build"
	},
	"dependencies": {
		"@kadence/icons": "git+https://github.com/stellarwp/kadence-icons.git#1.1.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/stellarwp/kadence-helpers.git"
	},
	"keywords": [
		"helpers",
		"kadence",
		"wordpress",
		"gutenberg"
	],
	"author": "kadencewp",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/stellarwp/kadence-helpers/issues"
	},
	"homepage": "https://github.com/stellarwp/kadence-helpers#readme",
	"peerDependencies": {
		"react": ">=17 <19",
		"react-dom": ">=17 <19"
	},
	"devDependencies": {
		"@babel/cli": "^7.26.0",
		"@babel/core": "^7.26.0",
		"@wordpress/babel-preset-default": "^8.0.0",
		"prettier": "^2.8.8",
		"rimraf": "^5.0.5"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	}
}
